{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true
    },
    "include": ["src/**/*.jsx"],
    "exclude": ["node_modules"]
  }
  